      "appliesTo": "all elements",
      "inherited": "yes",
      "percentages": "N/a",
      "media": "visual",
      "computedValue": "As specified",
      "canonicalOrder": "per grammar",
      "animatable": "No (see below)"
    },
    "font-variant-emoji": {
      "name": "font-variant-emoji",
      "value": "auto | text | emoji | unicode",
      "initial": "auto",
      "appliesTo": "all elements",
      "inherited": "yes",
      "percentages": "N/a",
      "media": "visual",
      "computedValue": "As specified",
      "canonicalOrder": "per grammar",
      "animatable": "No (see below)"
    }
  },
  "descriptors": {
    "font-family": {
      "name": "font-family",
      "for": "@font-palette-values",
      "value": "<‘font-family’>",
      "initial": "N/A"
    },
    "src": {
      "name": "src",
      "for": "@font-face",
      "value": "see prose",
      "initial": "N/A"
    },
    "font-style": {
      "name": "font-style",
      "for": "@font-face",
      "value": "auto | normal | italic | oblique [ <angle> | <angle> <angle> ] ?",
      "initial": "normal"
    },
    "font-weight": {
      "name": "font-weight",
      "for": "@font-face",
      "value": "auto | <font-weight-absolute> <font-weight-absolute>?",
      "initial": "normal"
    },
    "font-stretch": {
      "name": "font-stretch",
      "for": "@font-face",
      "value": "auto | <‘font-stretch’> <‘font-stretch’>?",
      "initial": "normal"
    },
    "unicode-range": {
      "name": "unicode-range",
      "for": "@font-face",
      "value": "<urange> #",
      "initial": "U+0-10FFFF"
    },
    "font-variant": {
      "name": "font-variant",
      "for": "@font-face",
      "value": "normal | none | [ <common-lig-values> || <discretionary-lig-values> || <historical-lig-values> || <contextual-alt-values> || stylistic(<feature-value-name>) || historical-forms || styleset(<feature-value-name>#) || character-variant(<feature-value-name>#) || swash(<feature-value-name>) || ornaments(<feature-value-name>) || annotation(<feature-value-name>) || [ small-caps | all-small-caps | petite-caps | all-petite-caps | unicase | titling-caps ] || <numeric-figure-values> || <numeric-spacing-values> || <numeric-fraction-values> || ordinal || slashed-zero || <east-asian-variant-values> || <east-asian-width-values> || ruby || [ sub | super ] ]",
      "initial": "normal"
    },
    "font-feature-settings": {
      "name": "font-feature-settings",
      "for": "@font-face",
      "value": "normal | <feature-tag-value> #",
      "initial": "normal"
    },
    "font-variation-settings": {
      "name": "font-variation-settings",
      "for": "@font-face",
      "value": "normal | [ <string> <number>] #",
      "initial": "normal"
    },
    "font-named-instance": {
      "name": "font-named-instance",
      "for": "@font-face",
      "value": "auto | <string>",
      "initial": "auto"
    },
    "font-display": {
      "name": "font-display",
      "for": "@font-feature-values",
      "value": "auto | block | swap | fallback | optional",
      "initial": "auto"
    },
    "font-language-override": {
      "name": "font-language-override",
      "for": "@font-face",
      "value": "normal | <string>",
      "initial": "normal"
    },
    "base-palette": {
      "name": "base-palette",
      "for": "@font-palette-values",
      "value": "<integer> | <string>",
      "initial": "N/A"
    },
    "override-color": {
      "name": "override-color",
      "for": "@font-palette-values",
      "value": "[ [ <string> | <integer> ] <color> ]#",
      "initial": "N/A"
    }
  },
  "valuespaces": {
    "<font-weight-absolute>": {
      "value": "[normal | bold | <number>]"
    },
    "<font-variant-css2>": {
      "value": "[normal | small-caps]"
    },
    "<font-stretch-css3>": {
      "value": "[normal | ultra-condensed | extra-condensed | condensed | semi-condensed | semi-expanded | expanded | extra-expanded | ultra-expanded]"
    },
    "<font-format>": {
      "value": "[<string> | woff | truetype | opentype | woff2 | embedded-opentype | svg]"
    },
    "<font-technology>": {
      "value": "[features | variations | color(<color-font-technology>) | palettes]"
    },
    "<color-font-technology>": {
      "value": "[COLR | SVG | sbix]"
    },
    "<feature-value-name>": {
      "value": "<ident>"
    },
    "<family-name>": {
      "prose": "The name of a font family, such as Helvetica or Verdana in the previous example."
    },
    "<feature-value-block>": {
      "value": "<feature-type> { <declaration-list> }"
    },
    "<feature-type>": {
      "value": "@stylistic | @historical-forms | @styleset | @character-variant | @swash | @ornaments | @annotation"
    },
    "<feature-value>": {
      "value": "<integer>+"
    },
    "<stylistic()>": {
      "value": "stylistic(<feature-value-name>)"
    },
    "<styleset()>": {
      "value": "styleset(<feature-value-name>#)"
    },
    "<character-variant()>": {
      "value": "character-variant(<feature-value-name>#)"
    },
    "<swash()>": {
      "value": "swash(<feature-value-name>)"
    },
    "<ornaments()>": {
      "value": "ornaments(<feature-value-name>)"
    },
    "<annotation()>": {
      "value": "annotation(<feature-value-name>)"
    },
    "<generic-family>": {
      "prose": "Each <generic-family> keyword represents a generic font choice, and behaves as an alias for one or more locally-installed fonts belonging to the specified generic font category. A <generic-family> can thus be used as a reliable fallback for when an author’s more specific font choices are not available. Authors are encouraged to append a generic font family as a last alternative for improved robustness. Note that <generic-family> keywords cannot be quoted (otherwise they are interpreted as a <family-name>). The following generic family keywords are defined: serif, sans-serif, cursive, fantasy, monospace, system-ui, emoji, math and fangsong. See §2.1.3 Generic font families."
    },
    "<palette-identifier>": {
      "prose": "This value identifies an author-define palette to use. Users can define a palette by using the @font-palette-values rule. If no applicable @font-palette-values rule is present, this value behaves as normal."
    }
  }
}
