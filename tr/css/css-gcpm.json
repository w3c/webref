{
  "spec": {
    "title": "CSS Generated Content for Paged Media Module",
    "url": "https://www.w3.org/TR/css-gcpm-3/"
  },
  "properties": {
    "string-set": {
      "name": "string-set",
      "value": "[[ <custom-ident> <content-list>] [, <custom-ident> <content-list>]* ] | none",
      "initial": "none",
      "appliesTo": "all elements, but not pseudo-elements",
      "inherited": "no",
      "media": "all",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "string-set",
        "stringSet"
      ]
    },
    "position": {
      "name": "position",
      "newValues": "running()",
      "media": "paged",
      "styleDeclaration": [
        "position"
      ]
    },
    "running": {
      "name": "running",
      "value": "<custom-ident>",
      "initial": "none",
      "appliesTo": "elements",
      "inherited": "no",
      "media": "all",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "running"
      ]
    },
    "content": {
      "name": "content",
      "newValues": "leader()",
      "media": "paged",
      "styleDeclaration": [
        "content"
      ]
    },
    "float": {
      "name": "float",
      "newValues": "footnote",
      "media": "paged",
      "styleDeclaration": [
        "float"
      ]
    },
    "footnote-display": {
      "name": "footnote-display",
      "value": "block | inline | compact",
      "initial": "block",
      "appliesTo": "elements",
      "inherited": "no",
      "media": "paged",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "footnote-display",
        "footnoteDisplay"
      ]
    },
    "footnote-policy": {
      "name": "footnote-policy",
      "value": "auto | line | block",
      "initial": "auto",
      "appliesTo": "elements",
      "inherited": "no",
      "media": "paged",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "footnote-policy",
        "footnotePolicy"
      ]
    },
    "bookmark-level": {
      "name": "bookmark-level",
      "value": "none | <integer>",
      "initial": "none",
      "appliesTo": "all elements",
      "inherited": "no",
      "media": "paged",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "bookmark-level",
        "bookmarkLevel"
      ]
    },
    "bookmark-label": {
      "name": "bookmark-label",
      "value": "<content-list> | none",
      "initial": "none",
      "appliesTo": "all elements",
      "inherited": "no",
      "media": "paged",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "bookmark-label",
        "bookmarkLabel"
      ]
    },
    "bookmark-state": {
      "name": "bookmark-state",
      "value": "open | closed",
      "initial": "open",
      "appliesTo": "block-level elements",
      "inherited": "no",
      "media": "paged",
      "computedValue": "specified value",
      "percentages": "N/A",
      "styleDeclaration": [
        "bookmark-state",
        "bookmarkState"
      ]
    }
  },
  "descriptors": {},
  "valuespaces": {
    "<content()>": {
      "value": "content([text | before | after | first-letter ])"
    },
    "<string()>": {
      "value": "string( <custom-ident> [ , [ first | start | last | first-except] ]? )"
    },
    "<running()>": {
      "value": "string( <custom-ident> )"
    },
    "<element()>": {
      "value": "string( <custom-ident> [ , [ first | start | last | first-except] ]? )"
    },
    "<nth()>": {
      "value": "( An+B [of <custom-ident>]?)"
    },
    "<leader()>": {
      "value": "leader( [dotted | solid | space] | <string>);"
    },
    "<target-counter()>": {
      "value": "target-counter( <url> , <custom-ident> [ , <counter-style> ]? )"
    },
    "<target-counters()>": {
      "value": "target-counter( <url> , <custom-ident> , <string> [ , <counter-style> ]? )"
    },
    "<target-text()>": {
      "value": "target-counter( <url> [ , [ content | before | after | first-letter] ]? )"
    }
  }
}
