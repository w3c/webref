{
  "spec": {
    "title": "Non-element Selectors Module Level 1",
    "url": "https://www.w3.org/TR/2019/NOTE-selectors-nonelement-1-20190402/"
  },
  "properties": {},
  "descriptors": {},
  "valuespaces": {
    "<::attr()>": {
      "value": "::attr( <namespace-attr>? )"
    },
    "<namespace-attr>": {
      "value": "[ <na-prefix>? '|' ]? <na-name>"
    },
    "<na-prefix>": {
      "value": "<ident> | '*'"
    },
    "<na-name>": {
      "value": "<ident> | '*'"
    }
  }
}
