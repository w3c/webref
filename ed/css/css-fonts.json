  },
  "descriptors": {
    "font-family": [
      {
        "name": "font-family",
        "for": "@font-face",
        "value": "<family-name>",
        "initial": "N/A"
      },
      {
        "name": "font-family",
        "for": "@font-palette-values",
        "value": "<'font-family'>",
        "initial": "N/A"
      }
    ],
    "src": [
      {
        "name": "src",
        "for": "@font-face",
        "value": "see prose",
        "initial": "N/A"
      }
    ],
    "font-style": [
      {
        "name": "font-style",
        "for": "@font-face",
        "value": "auto | normal | italic | oblique [ <angle>{1,2} ]?",
        "initial": "auto"
      }
    ],
    "font-weight": [
      {
        "name": "font-weight",
        "for": "@font-face",
        "value": "auto | <font-weight-absolute>{1,2}",
        "initial": "auto"
      }
    ],
    "font-stretch": [
      {
        "name": "font-stretch",
        "for": "@font-face",
        "value": "auto | <'font-stretch'>{1,2}",
        "initial": "auto"
      }
    ],
