      }
    ],
    "font-named-instance": [
      {
        "name": "font-named-instance",
        "for": "@font-face",
        "value": "auto | <string>",
        "initial": "auto"
      }
    ],
    "font-display": [
      {
        "name": "font-display",
        "for": "@font-face",
        "value": "auto | block | swap | fallback | optional",
        "initial": "auto"
      },
      {
        "name": "font-display",
        "for": "@font-feature-values",
        "value": "auto | block | swap | fallback | optional",
        "initial": "auto"
      }
    ],
    "font-language-override": [
      {
        "name": "font-language-override",
        "for": "@font-face",
        "value": "normal | <string>",
        "initial": "normal"
      }
    ],
    "ascent-override": [
