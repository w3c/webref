{
  "spec": {
    "title": "Scroll-linked Animations",
    "url": "https://drafts.csswg.org/scroll-animations-1/"
  },
  "properties": {},
  "descriptors": {
    "source": [
      {
        "name": "source",
        "for": "@scroll-timeline",
        "value": "selector( <id-selector> ) | auto | none",
        "initial": "auto"
      }
    ],
    "orientation": [
      {
        "name": "orientation",
        "for": "@scroll-timeline",
        "value": "auto | block | inline | horizontal | vertical",
        "initial": "auto"
      }
    ],
    "scroll-offsets": [
      {
        "name": "scroll-offsets",
        "for": "@scroll-timeline",
        "value": "[ none | <scroll-timeline-offset> ]#",
        "initial": "none"
      }
    ]
  },
  "valuespaces": {
    "<scroll-timeline-offset>": {
      "value": "auto | <length-percentage> | <element-offset>"
    },
    "<element-offset>": {
      "value": "selector( <id-selector> ) [<element-offset-edge> || <number>]?"
    },
    "<element-offset-edge>": {
      "value": "start | end"
    }
  }
}
